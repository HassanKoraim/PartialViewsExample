
<h1>Home</h1>

<button class="button button-blue-back" type="button" id="button-load">load Programming 
    Languges</button>

<div class="box programming-languages-content"> 

</div>
<script>
    document.querySelector("#button-load").addEventListener("click", async function () {
        var resonse = await fetch("Programming-Languages");
        var languages = await resonse.text();
        document.querySelector(".programming-languages-content").innerHTML = languages;
    });
</script>

